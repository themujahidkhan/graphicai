{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
      ]
    }
  ],
  "redirects": [
    { "source": "http://:domain/:path*", "destination": "https://:domain/:path*", "permanent": true }
  ]
}