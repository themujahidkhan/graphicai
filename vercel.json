{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
      ]
    }
  ],
  "redirects": [
    { "source": "/:path((?!api/).)*", "destination": "https://app.graphicai.design/:path*", "permanent": true }
  ]
}